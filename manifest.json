{
    "name": "Trello Scrum",
    "description": "...",
    "version": "0.3",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "https://trello.com/*"
        ],
        "js": [
            "src/libs/jquery/dist/jquery.min.js",
            "src/js/infra/exists.js",
            "src/js/infra/observer.js",
            "src/js/infra/string-format.js",
            "src/js/infra/queries/get-lists.js",
            "src/js/entities/card.js",
            "src/js/entities/list.js",
            "src/js/app.js",
            "src/js/main.js"
        ],
        "css": [
            "src/css/style.css"
        ]
    }],
    "icons": {
        "128": "src/img/icon_128x128.png"
    },
    "web_accessible_resources": [],
    "permissions": [
        "storage"
    ]
}